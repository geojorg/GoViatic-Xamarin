@model GoViatic.Web.Models.TripViewModel
@{
    ViewData["Title"] = "Create Trip";
}

<h2>Add New Trip</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateTrip" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="TravelerId"/>
           
            <partial name="_Trip" />

            <div class="form-group">
                <input type="submit" value="CreateTrip" class="btn btn-success" />
                <a asp-action="DetailsTraveler" asp-route-id="@Model.TravelerId" class="btn btn-danger">Back To Trips</a>
            </div>
        </form>
    </div>
</div>